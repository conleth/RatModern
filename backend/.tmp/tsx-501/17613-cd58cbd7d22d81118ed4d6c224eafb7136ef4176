)score+=1;if(!answers.modernFramework)score+=1;const level=determineLevel(score);const applicationType=determineApplicationType(answers);const discipline=determineDiscipline(answers,role);const technology=determineTechnology(answers);const notes=collectNotes(answers);const recommendedCategories=collectCategories(answers);return{level,applicationType,discipline,technology,notes,recommendedCategories}}__name(generateRecommendations,\"generateRecommendations\");export{QUESTIONNAIRE_QUESTIONS,generateRecommendations};\n","warnings":[],"map":{"version":3,"mappings":"kHAqBO,MAAM,wBAAmD,CAC9D,CACE,GAAI,kBACJ,KAAM,4EACN,SAAU,oFACV,KAAM,CAAC,OAAQ,WAAW,CAC5B,EACA,CACE,GAAI,YACJ,KAAM,0FACN,SAAU,6EACV,KAAM,CAAC,OAAQ,SAAS,CAC1B,EACA,CACE,GAAI,mBACJ,KAAM,8DACN,KAAM,CAAC,MAAM,CACf,EACA,CACE,GAAI,mBACJ,KAAM,8EACN,KAAM,CAAC,YAAY,CACrB,EACA,CACE,GAAI,eACJ,KAAM,iEACN,KAAM,CAAC,MAAM,CACf,EACA,CACE,GAAI,uBACJ,KAAM,oEACN,KAAM,CAAC,aAAa,CACtB,EACA,CACE,GAAI,kBACJ,KAAM,wEACN,SAAU,4EACV,KAAM,CAAC,OAAO,CAChB,EACA,CACE,GAAI,eACJ,KAAM,uDACN,KAAM,CAAC,UAAU,CACnB,EACA,CACE,GAAI,aACJ,KAAM,yEACN,KAAM,CAAC,UAAU,CACnB,CACF,EAEA,SAAS,eAAe,MAA0B,CAChD,GAAI,OAAS,EAAG,CACd,MAAO,IACT,CACA,GAAI,OAAS,EAAG,CACd,MAAO,IACT,CACA,MAAO,IACT,CARS,wCAUT,SAAS,yBAAyB,QAAgD,CAChF,GAAI,QAAQ,aAAc,CACxB,MAAO,QACT,CACA,GAAI,QAAQ,WAAY,CACtB,MAAO,KACT,CACA,MAAO,KACT,CARS,4DAUT,SAAS,oBACP,QACA,KACqB,CACrB,GAAI,QAAQ,aAAc,CACxB,MAAO,QACT,CACA,GAAI,QAAQ,WAAY,CACtB,MAAO,SACT,CACA,GAAI,OAAS,YAAa,CACxB,OAAO,QAAQ,iBAAmB,YAAc,SAClD,CACA,GAAI,OAAS,SAAU,CACrB,MAAO,aACT,CACA,GAAI,OAAS,aAAe,OAAS,YAAa,CAChD,MAAO,mBACT,CACA,MAAO,iBACT,CApBS,kDAsBT,SAAS,oBAAoB,QAAsD,CACjF,GAAI,QAAQ,aAAc,CACxB,MAAO,QACT,CACA,GAAI,QAAQ,WAAY,CACtB,MAAO,MACT,CACA,MAAO,KACT,CARS,kDAUT,SAAS,aAAa,QAAyC,CAC7D,MAAM,MAAkB,CAAC,EAEzB,GAAI,QAAQ,gBAAiB,CAC3B,MAAM,KAAK,8EAA8E,CAC3F,CAEA,GAAI,QAAQ,UAAW,CACrB,MAAM,KAAK,+EAA+E,CAC5F,CAEA,GAAI,QAAQ,iBAAkB,CAC5B,MAAM,KAAK,0EAA0E,CACvF,CAEA,GAAI,CAAC,QAAQ,gBAAiB,CAC5B,MAAM,KAAK,wEAAwE,CACrF,CAEA,GAAI,QAAQ,qBAAsB,CAChC,MAAM,K